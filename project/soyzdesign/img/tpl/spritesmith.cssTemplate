{{#items}}

.icon-{{name}}{
	position: relative;
}

.icon-{{name}}:before{
	content:"";
	display: inline-block;
	background-image: url({{{escaped_image}}});
	background-position: {{px.offset_x}} {{px.offset_y}};
	width: {{px.width}};
	height: {{px.height}};
	position: absolute;
	top: 0;
	left: 0;
}
{{/items}}